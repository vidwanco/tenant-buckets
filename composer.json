{
    "name": "vidwan/tenant-buckets",
    "description": "Automatically Provision AWS S3 Buckets for tenants.",
    "keywords": [
        "Vidwan Co.",
        "tenant-buckets",
        "Tenancy For Laravel",
        "tenancyforlaravel"
    ],
    "homepage": "https://github.com/vidwan-co/tenant-buckets",
    "license": "MIT",
    "authors": [
        {
            "name": "Shashwat Mishra",
            "email": "shashwat@vidwan.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "aws/aws-sdk-php": "~3.0",
        "stancl/tenancy": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20|^2.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Vidwan\\TenantBuckets\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vidwan\\TenantBuckets\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
